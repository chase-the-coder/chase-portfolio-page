{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
        "rules": {
          "no-console": "off",
          "comma-dangle": "off",
          "quotes": "off",
          "react/prop-types": 0,
          "arrow-body-style": 0,
          "space-before-function-paren": 0
        },
        "env": {
          "browser": true
        }
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
    }
}
